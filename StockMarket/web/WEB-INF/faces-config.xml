<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <managed-bean> 
        <managed-bean-name> companyManager </managed-bean-name>
        <managed-bean-class> entity.CompanyManager </managed-bean-class>
        <managed-bean-scope> request </managed-bean-scope>
        <managed-property>
            <property-name>activeCompanySymbol</property-name>
            <value>#{param.symbol}</value>
        </managed-property>
    </managed-bean>
    <managed-bean>
        <managed-bean-name> sectorInfo </managed-bean-name>
        <managed-bean-class> entity.SectorInfo </managed-bean-class>
        <managed-bean-scope> request </managed-bean-scope>
        <managed-property>
            <property-name>companyManager</property-name>
            <value>#{companyManager}</value>
        </managed-property>
    </managed-bean>
</faces-config>
